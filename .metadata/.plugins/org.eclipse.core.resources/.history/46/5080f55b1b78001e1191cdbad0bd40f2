package fundamentals;

public class DataTypeStorage {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int min = Integer.MIN_VALUE;   // will store min value that a int data type can store
		int max = Integer.MAX_VALUE;  //will store max value that a int data type can store
		System.out.println(min+"      --------------      "+max);

		char ch = 'a';
		// a      -    97
		//b       -   98
		//A      -   65
		//'0'   -   48
	}

}
